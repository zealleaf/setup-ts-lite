    "lint:fix": "nr lint -- --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "esno src/index.ts",
    "tc": "nr typeCheck",
    "test": "vitest",
    "typeCheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@types/node": "^18.7.18",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.2",
    "vitest": "^0.23.4"
  },
  "packageManager": "pnpm@7.12.0"
